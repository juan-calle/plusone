{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "core-app",
  "projects": {
    "types": {
      "root": "libs/types",
      "sourceRoot": "libs/types/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/types/**/*.ts"]
          }
        }
      }
    },
    "core-app": {
      "root": "apps/core-app",
      "sourceRoot": "apps/core-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/core-app",
            "main": "apps/core-app/src/main.ts",
            "tsConfig": "apps/core-app/tsconfig.app.json",
            "assets": ["apps/core-app/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/core-app/src/environments/environment.ts",
                  "with": "apps/core-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "core-app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/core-app/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/core-app"],
          "options": {
            "jestConfig": "apps/core-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "fetch-app": {
      "root": "apps/fetch-app",
      "sourceRoot": "apps/fetch-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/fetch-app",
            "main": "apps/fetch-app/src/main.ts",
            "tsConfig": "apps/fetch-app/tsconfig.app.json",
            "assets": ["apps/fetch-app/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/fetch-app/src/environments/environment.ts",
                  "with": "apps/fetch-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "fetch-app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/fetch-app/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/fetch-app"],
          "options": {
            "jestConfig": "apps/fetch-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "discovery-app": {
      "root": "apps/discovery-app",
      "sourceRoot": "apps/discovery-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/discovery-app",
            "main": "apps/discovery-app/src/main.ts",
            "tsConfig": "apps/discovery-app/tsconfig.app.json",
            "assets": ["apps/discovery-app/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/discovery-app/src/environments/environment.ts",
                  "with": "apps/discovery-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "discovery-app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/discovery-app/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/discovery-app"],
          "options": {
            "jestConfig": "apps/discovery-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "fetch": {
      "root": "libs/fetch",
      "sourceRoot": "libs/fetch/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/fetch/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/fetch"],
          "options": {
            "jestConfig": "libs/fetch/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
