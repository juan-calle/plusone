{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "core",
  "projects": {
    "discovery": {
      "root": "apps/discovery",
      "sourceRoot": "apps/discovery/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/discovery",
            "main": "apps/discovery/src/main.ts",
            "tsConfig": "apps/discovery/tsconfig.app.json",
            "assets": ["apps/discovery/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/discovery/src/environments/environment.ts",
                  "with": "apps/discovery/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "discovery:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/discovery/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/discovery"],
          "options": {
            "jestConfig": "apps/discovery/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core": {
      "root": "apps/core",
      "sourceRoot": "apps/core/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/core",
            "main": "apps/core/src/main.ts",
            "tsConfig": "apps/core/tsconfig.app.json",
            "assets": ["apps/core/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/core/src/environments/environment.ts",
                  "with": "apps/core/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "core:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/core/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/core"],
          "options": {
            "jestConfig": "apps/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "types": {
      "root": "libs/types",
      "sourceRoot": "libs/types/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/types/**/*.ts"]
          }
        }
      }
    },
    "fetch": {
      "root": "apps/fetch",
      "sourceRoot": "apps/fetch/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/fetch",
            "main": "apps/fetch/src/main.ts",
            "tsConfig": "apps/fetch/tsconfig.app.json",
            "assets": ["apps/fetch/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/fetch/src/environments/environment.ts",
                  "with": "apps/fetch/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "fetch:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/fetch/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/fetch"],
          "options": {
            "jestConfig": "apps/fetch/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
